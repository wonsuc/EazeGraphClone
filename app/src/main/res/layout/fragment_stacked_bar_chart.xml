<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.w.eazegraphclone.ui.MainActivity">

    <com.example.w.eazegraphclone.charts.StackedBarChart
        android:id="@+id/stackedbarchart"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:background="@android:color/holo_red_light"
        app:egFixedBarWidth="true"
        app:egShowValues="true"
        app:egBarWidth="30dp"
        app:egLegendHeight="30dp"
        app:egShowDecimal="false"/>

</FrameLayout>
